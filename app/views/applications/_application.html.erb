<tr id="application_<%= application.id %>">
  <td><%= link_to application.name, application %></td>
  <td><%= link_to application.script_file_name, application.script.url %></td>
  <td><%= application.repository %></td>
  <td>
    <%= link_to 'Edit', edit_application_path(application) %>
    <%= link_to 'Destroy', application, method: :delete, data: { confirm: 'Are you sure?' } %>
  </td>
</tr>