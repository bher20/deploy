<!DOCTYPE html>
<html>
<head>
  <title><%= t('deploy.deploy') %></title>
  <%= stylesheet_link_tag    'application', :media => 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id='header'>
  <%= render 'layouts/header' %>
</div>
<div id='main'>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  <%= yield %>
</div>
<div id='footer'>
  <%= render 'layouts/footer' %>
</div>
</body>
</html>